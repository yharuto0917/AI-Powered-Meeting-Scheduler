{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "functions": {
    "source": "functions",
    "runtime": "python311"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "emulators": {
    "auth": {
      "port": 9199
    },
    "functions": {
      "port": 5101
    },
    "firestore": {
      "port": 8180
    },
    "hosting": {
      "port": 3100
    },
    "ui": {
      "enabled": true,
      "port": 4100
    },
    "hub": {
      "port": 4401
    },
    "singleProjectMode": true
  }
}