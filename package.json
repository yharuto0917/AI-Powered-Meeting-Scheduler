{
  "name": "meeting-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:emulator": "cp .env.local.emulator .env.local && concurrently \"npm run emulator\" \"npm run dev\"",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "emulator": "firebase emulators:start --only auth,firestore,functions",
